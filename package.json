{
  "name": "resumerag",
  "version": "1.0.0",
  "description": "Semantic Resume Search & Job Matching System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npm run copy-files",
    "copy-files": "echo 'Build complete'",
    "start": "node dist/index.js",
    "migrate": "ts-node src/database/migrate.ts",
    "migrate:prod": "node dist/database/migrate.js",
    "seed": "ts-node src/database/seed.ts",
    "seed:prod": "node dist/database/seed.js",
    "deploy": "npm run build && npm run migrate:prod && npm start",
    "test": "echo \"Tests not implemented\" && exit 0"
  },
  "keywords": ["resume", "rag", "ai", "job-matching"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "adm-zip": "^0.5.10",
    "openai": "^4.20.1",
    "node-cache": "^5.1.2",
    "dotenv": "^16.3.1",
    "crypto": "^1.0.1",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.10.9",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/pdf-parse": "^1.1.4",
    "@types/node": "^20.10.5",
    "@types/adm-zip": "^0.5.5",
    "ts-node-dev": "^2.0.0"
  }
}
